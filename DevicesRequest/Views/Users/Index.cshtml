@model IEnumerable<DevicesRequest.Models.User>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>List User</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="row panel invoice-body">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Department)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Position)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.FirstNameEn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastNameEn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JobNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telephon)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Department.NameEn)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Position.NameEn)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.FirstNameEn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastNameEn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.JobNumber)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Telephon)
                </td>


                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.UserId }) |
                    @Html.ActionLink("Details", "Details", new { id = item.UserId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.UserId })
                </td>
            </tr>
        }

    </table>

</div>