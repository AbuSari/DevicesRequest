@model DevicesRequest.Models.User

@{

    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="col-md-12 detailed alert alert-warning">
    <h3 class="centered">User Details</h3>
    <hr />
    <div class="col-md-3 right-divider  mt">
        <p>
            <strong>Arabic Name </strong> <br>
            @Html.DisplayFor(model => model.FirstNameAr)
            @Html.DisplayFor(model => model.LastNameAr)
            <br>
        </p>
        <br>
        <p>
            <strong>English Name</strong> <br>
            @Html.DisplayFor(model => model.FirstNameEn)
            @Html.DisplayFor(model => model.LastNameEn)
            <br>
        </p>
        <br>
        <p>
            <strong>@Html.DisplayNameFor(model => model.JobNumber)</strong> <br>
            @Html.DisplayFor(model => model.JobNumber)
            <br>
        </p>

    </div>
    <div class="col-md-3 right-divider mt">
        <p>
            <strong>@Html.DisplayNameFor(model => model.UserEmail)</strong> <br>
            @Html.DisplayFor(model => model.UserEmail)
            <br>
        </p>
        <br>
        <p>
            <strong>@Html.DisplayNameFor(model => model.Telephon)</strong> <br>
            @Html.DisplayFor(model => model.Telephon)
            <br>
        </p>
        <br>
        <p>
            <strong>@Html.DisplayNameFor(model => model.Mobile)</strong> <br>
            @Html.DisplayFor(model => model.Mobile)
            <br>
        </p>
    </div>
    <div class="col-md-3 right-divider mt">
        <p>
            <strong>@Html.DisplayNameFor(model => model.Department)</strong> <br>
            @Html.DisplayFor(model => model.Department.NameEn)
            <br>
        </p>
        <br>
        <p>
            <strong>@Html.DisplayNameFor(model => model.Level)</strong> <br>
            @Html.DisplayFor(model => model.Level.NameEn)
            <br>
        </p>
        <br>
        <p>
            <strong>@Html.DisplayNameFor(model => model.RoomNo)</strong> <br>
            @Html.DisplayFor(model => model.RoomNo)
            <br>
        </p>
    </div>
    <div class="col-md-3 right-divider mt">

        <p>
            <strong>@Html.DisplayNameFor(model => model.CreatedDate)</strong> <br>
            @Html.DisplayFor(model => model.CreatedDate)
            <br>
        </p>
        <br>
        <p>
            <strong>@Html.DisplayNameFor(model => model.LastUpdateBy)</strong> <br>
            @Html.DisplayFor(model => model.LastUpdateBy)
            <br>
        </p>
        <br />
        <p>
            <strong>@Html.DisplayNameFor(model => model.LastUpdateDate)</strong> <br>
            <b>@Html.DisplayFor(model => model.LastUpdateDate)</b>
            <br>
        </p>
    </div>

</div>

<p>
    @Html.ActionLink(" ", "Index", new { id = Model.UserId }, new { @class = "btn btn-default btn-sm fa fa-angle-double-left tooltips", data_placement = "top", title = "Back", @style = "line-height: unset" })
    @Html.ActionLink(" ", "Edit", new { id = Model.UserId }, new { @class = "btn btn-primary btn-sm fa fa-pencil tooltips", data_placement = "top", title = "Edit", @style = "line-height: unset" })
</p>

